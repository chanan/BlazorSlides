@using BlazorSlides.Internal.Components.Fragments
@switch (FragmentStyle)
{
    case FragmentStyle.Default:
        <Default>@ChildContent</Default>
        break;
    case FragmentStyle.Grow:
        <Grow>@ChildContent</Grow>
        break;
    case FragmentStyle.Shrink:
        <Shrink>@ChildContent</Shrink>
        break;
    case FragmentStyle.ZoomIn:
        <ZoomIn>@ChildContent</ZoomIn>
        break;
    case FragmentStyle.FadeDown:
        <FadeDown>@ChildContent</FadeDown>
        break;
    case FragmentStyle.FadeLeft:
        <FadeLeft>@ChildContent</FadeLeft>
        break;
    case FragmentStyle.FadeRight:
        <FadeLeft>@ChildContent</FadeLeft>
        break;
    case FragmentStyle.FadeUp:
        <FadeUp>@ChildContent</FadeUp>
        break;
    case FragmentStyle.FadeInThenOut:
        <FadeInThenOut>@ChildContent</FadeInThenOut>
        break;
    case FragmentStyle.FadeInThenSemiOut:
        <FadeInThenSemiOut>@ChildContent</FadeInThenSemiOut>
        break;
    case FragmentStyle.HighlightRed:
        <HighlightRed>@ChildContent</HighlightRed>
        break;
    case FragmentStyle.HighlightGreen:
        <HighlightGreen>@ChildContent</HighlightGreen>
        break;
    case FragmentStyle.HighlightBlue:
        <HighlightBlue>@ChildContent</HighlightBlue>
        break;
    case FragmentStyle.Strike:
        <Strike>@ChildContent</Strike>
        break;
}